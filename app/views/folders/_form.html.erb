<%= form_with model: @folder do |f| %>
  <div>
        <%= f.label :name %>
        <%= f.text_field :name %>
  </div>
 <br><br><br><br>
  <div>
    <%= f.label :description %>
    <%= f.text_area :description%>
  </div>
   <br><br><br><br>
  <div>
    <%= f.label :tag %>
    <%= f.text_area :tag%>
  </div>
 <br><br><br><br>
  <div>
    <%= f.label :photos%>
    <%= f.file_field :photos, multiple: true  %>
  </div>
 <br><br><br><br>
  <%= f.submit %>
<% end %> 
 <br><br><br><br>
